<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="cn.hncu.domain.Book">
	
	<insert id="insert" parameterType="cn.hncu.domain.Book">
		insert into 
		book( name, price, sid ) 
		values( #{name}, #{price}, #{student.id} )
	</insert>
	
</mapper>
  